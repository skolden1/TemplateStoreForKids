@page
@model RegisterConfirmationModel
@{
    ViewData["Title"] = "Registrerings bekräftelse";
}

<h1>@ViewData["Title"]</h1>
@{
    if (@Model.DisplayConfirmAccountLink)
    {
<p>
     <a id="confirm-link" href="@Model.EmailConfirmationUrl"><strong>Bekräfta ditt konto genom att klicka här</strong></a>
</p>
    }
    else
    {
<p>
        Please check your email to confirm your account.
</p>
    }
}

